{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "google",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "object-curly-spacing": ["error", "always"],
    "max-len": ["error", { "code": 100 }],
    "comma-dangle": ["off"],
    "indent": ["off"]
  }
}
